u1<-(1:10)

for(i in 1:10){
  u1[i]<-u1[i]+2
  print(u1[i])
}

s<-5
while(s<6){s<-s*2;print(s)}

s<-1
while(s<5){s<-s+1;if(s==4) break;print(s);}
 
s<-1
while(s<5){s<-0+1;if(s==2) break;print(s);}
#corre todos los valores por que no encuentra el 2
s<-2
while(s<3){s<-s+1;if(s==2) next;print(s);}

